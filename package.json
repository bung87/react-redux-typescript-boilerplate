{
  "name": "react-hooks-redux-typescript-boilerplate",
  "version": "1.1.1",
  "private": true,
  "description": "A frontend boilerplate with React hooks, Redux and Typescript",
  "keywords": [
    "react",
    "hooks",
    "redux",
    "typescript",
    "boilerplate",
    "todomvc"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "format": "prettier -w src",
    "analyze": "source-map-explorer 'build/*.js'",
    "supportedBrowsers": "browserslist-useragent-regexp --allowHigherVersions -o src/supportedBrowsers.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dashboard -- webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
    "build": "webpack --progress"
  },
  "license": "MIT",
  "devDependencies": {
    "@ad2302/browserslist-useragent-regexp": "^3.1.2",
    "@babel/core": "^7.15.0",
    "@hot-loader/react-dom": "^16.14.0",
    "@juzibot/eslint-config-react": "^1.0.12",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.3",
    "@types/mini-css-extract-plugin": "^2.2.0",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/webpack": "^4.41.30",
    "@types/webpack-env": "^1.16.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "gh-pages": "^2.1.1",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "json5": "^2.2.0",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.2",
    "postcss-url": "^10.1.3",
    "prettier": "^2.3.2",
    "react-hot-loader": "^4.13.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.8.0",
    "webpack-dashboard": "^3.3.5",
    "webpack-dev-server": "^4.0.0"
  },
  "dependencies": {
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "less": "^4.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "redux": "^4.1.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0"
